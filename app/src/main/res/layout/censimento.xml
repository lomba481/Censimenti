<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".CensimentiInterni">

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/indietroBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:rotationY="180"
            android:src="@drawable/baseline_arrow_forward_24"
            app:backgroundTint="@color/blue_restart"
            app:maxImageSize="30dp"
            app:tint="@color/white"
            tools:ignore="SpeakableTextPresentCheck">

        </com.google.android.material.floatingactionbutton.FloatingActionButton>

        <Switch
            android:id="@+id/switchBtn"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="10dp"
            android:checked="false"
            android:padding="2dp"
            android:thumb="@drawable/baseline_brightness_1_24"
            android:track="@drawable/track_switch"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="MissingConstraints,UseSwitchCompatOrMaterialXml" />


        <com.getbase.floatingactionbutton.FloatingActionsMenu
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="5dp"
            app:fab_labelStyle="@color/white"
            app:fab_addButtonColorNormal="@color/orange_restart"
            app:fab_addButtonColorPressed="@color/white">

            <com.getbase.floatingactionbutton.FloatingActionButton
                android:id="@+id/lampadaFAB"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:fab_colorNormal="@color/gray"
                app:fab_colorPressed="@color/white"
                app:fab_icon="@drawable/baseline_light_24"
                app:fab_size="mini"
                app:fab_title="Lampada" />

            <com.getbase.floatingactionbutton.FloatingActionButton
                android:id="@+id/localeFAB"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:fab_colorNormal="@color/gray"
                app:fab_colorPressed="@color/white"
                app:fab_icon="@drawable/baseline_square_foot_24"
                app:fab_size="mini"
                app:fab_title="Locale" />


        </com.getbase.floatingactionbutton.FloatingActionsMenu>

        
    </RelativeLayout>






</androidx.constraintlayout.widget.ConstraintLayout>